{
  "name": "frontend-storybox",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "serve -s dist -l 80",
    "start:pm2": "pm2 start 'serve -s dist -l 80' --name StoryBox-Client --watch",
    "start:iptables": "sudo iptables -t nat -A PREROUTING -p tcp --dport 80 -j REDIRECT --to-port 80",
    "build:prod": "tsc -b && vite build --mode production",
    "deploy": "cp .envprod .env && npm run build:prod && start:iptables && npm run start:pm2 ",
    "predeploy": "npm run build",
    "deployCheck": "gh-pages -d dist",
    "init": "npm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@hookform/resolvers": "^4.1.3",
    "@react-oauth/google": "^0.12.1",
    "axios": "^1.8.1",
    "bootstrap": "^5.3.3",
    "font-awesome": "^4.7.0",
    "iptables": "^0.0.4",
    "pm2": "^5.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-easy-crop": "^5.4.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.3.0",
    "serve": "^14.2.4",
    "socket.io-client": "^4.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
